function BuscaConAlias($txt, &$Campos=NULL){
global $_Form, $HaySelect, $FormDel, $_DBGROUPBY;
if( strlen($txt) == 0 ) return $txt;
$SubSelect = array(); $n=0;
while( (substr_count(strtoupper($txt), ' IN(')>0 || substr_count(strtoupper($txt), ' IN (')>0) && substr_count($txt, ')')>0 ){
$i = max(strpos(strtoupper($txt), ' IN('), strpos(strtoupper($txt), ' IN ('));
if( $i>-1 ){
$f = strpos(strtoupper($txt), ')', $i);
$SubSelect[$n] = substr($txt, $i, $f-$i+1);
$txt = str_replace($SubSelect[$n], '{#SubSelect#'.$n.'}', $txt);
}
$n++;
}
$txt = trim($txt);
if( $txt[0]=='(' ) $txt = '( '.substr($txt,1);
$txt = _FieldInWhere($txt, true);
if( !$HaySelect ){
if( count($SubSelect)>0 ) for($n=0; $n<count($SubSelect); $n++) $txt = str_replace( '{#SubSelect#'.$n.'}', $SubSelect[$n], $txt );
return $txt;
}
$TipoCondi = array( '<>','>=','<=','!=','=','>','<',' like',' is ',' ' );
$txt = ' '.$txt;
for( $n=0; $n<count($_Form); $n++ ){
for( $i=0; $i<count($TipoCondi); $i++ ){
if( substr_count( $txt, ' '.$_Form[$n][1].$TipoCondi[$i] ) > 0 ){
$txt = str_replace( ' '.$_Form[$n][1].$TipoCondi[$i], ' A.'.$_Form[$n][1].$TipoCondi[$i], $txt );
}
if( substr_count( $txt, '('.$_Form[$n][1].$TipoCondi[$i] ) > 0 ){
$txt = str_replace( '('.$_Form[$n][1].$TipoCondi[$i], ' (A.'.$_Form[$n][1].$TipoCondi[$i], $txt );
}
}
}
for( $n=0; $n<count($FormDel); $n++ ){
for( $i=0; $i<count($TipoCondi); $i++ ){
if( substr_count( $txt, ' '.$FormDel[$n][1].$TipoCondi[$i] ) > 0 ){
$txt = str_replace( ' '.$FormDel[$n][1].$TipoCondi[$i], ' A.'.$FormDel[$n][1].$TipoCondi[$i], $txt );
}
if( substr_count( $txt, '('.$FormDel[$n][1].$TipoCondi[$i] ) > 0 ){
$txt = str_replace( '('.$FormDel[$n][1].$TipoCondi[$i], '(A.'.$FormDel[$n][1].$TipoCondi[$i], $txt );
}
}
}
if( trim($txt)!='' && substr(trim($txt),0,1)!='(' && substr(trim($txt),1,1)!='.' && substr(trim($txt),2,1)!='.' ){
if( substr(trim($txt),0,7) == 'extend(' ){
$txt = str_replace( 'extend(', 'extend( A.', $txt );
}else if( substr(trim($txt),0,9) == 'group by ' ){
}else{
$txt = ' A.'.trim($txt);
}
}
if( count($SubSelect)>0 ) for($n=0; $n<count($SubSelect); $n++) $txt = str_replace( '{#SubSelect#'.$n.'}', $SubSelect[$n], $txt );
$txt = str_replace( 'A.(', '(', $txt );
return rtrim($txt);
}
function _explodePorComa($txt){
$sql = trim($txt);
$txt = '';
$IniParentesis = 0;
$IniComillaSencilla = 0;
$IniComillaDoble = 0;
$Dim = array();
for( $n=0; $n<strlen($sql); $n++ ){
$c = substr($sql,$n,1);
