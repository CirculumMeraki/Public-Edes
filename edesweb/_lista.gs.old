if( substr($buffer,-1)!=';' ) $buffer .= ';';
if( !preg_match('/PDF_/', $buffer) ) $buffer = str_replace('$', '$PDF_', $buffer);
$GLOBALS["_PDFVAR"][] = $buffer;
}
