$IniParentesis++;
break;
case ')':
if( $IniComillaSencilla || $IniComillaDoble ) break;
$IniParentesis--;
break;
case "'":
if( $IniComillaDoble ) break;
if( $IniComillaSencilla > 0 ){
if( substr($sql,$n-1,1)==chr(92) ) break;
$IniComillaSencilla--;
break;
}
$IniComillaSencilla++;
break;
case '"':
if( $IniComillaSencilla ) break;
if( $IniComillaDoble ){
if( substr($sql,$n-1,1)==chr(92) ) break;
$IniComillaDoble--;
break;
}
$IniComillaDoble++;
break;
default:
}
$txt .= $c;
}
if( trim($txt)<>"" ) $Dim[] = trim($txt);
return $Dim;
}
