function Ejecutar(){
if( eGF('FILTRO')!='' ){
if( eGF('TABLA').indexOf(',')>-1 ){
ePE('FILTRO','El filtro es sólo para una tabla');
eShowError();
return;
}
if( eGF('ADD_DELETE')=='B' ){
if( eGF('FILTRO')!='' && (eGF('FILTRO')*1)==eGF('FILTRO') && eGF('FILTRO') > 0 ){
}else{
ePE('FILTRO','Es incompatible el campo "Filtro" y la opción de borrar la base de datos');
eShowError();
return;
}
}
}
DGI("TABLA").value = DGI("TABLA").value.replace(/\s/g,'');
if( DGI("TABLA").value!='' ){
eDisableButton(0);
setTimeout('_Ejecutar("'+eGF('UPPER_LOWER')+'")',1);
}
eClearEvent();
}