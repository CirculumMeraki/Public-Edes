function NomUsuarios(){
$p = array();
$t = array();
$di = opendir( '../_d_/log/' );
while( $file = readdir( $di ) ){
if( ($file != '.') && ($file != '..') ){
if( !is_dir('../_d_/log/'.$file) ){
if( substr($file,0,9)=='opstatus.' ){
list( ,$nUser ) = explode('.',$file);
qQuery( "select user_name from gs_user where cd_gs_user='{$nUser}'");
list( $Nombre ) = qRow();
$t[] = "{$Nombre}|{$nUser}";
}
}
}
}
closedir( $di );
sort($t);
for( $n=0; $n<count($t); $n++ ){
$tmp = explode( '|', $t[$n] );
$p[] = array( $tmp[1], $tmp[0] );
}
return $p;
}
