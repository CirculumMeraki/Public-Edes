function CalculaTiempo( $Inicio, $Final ){
list( $h, $m ) = explode(':',$Inicio);
$MinDesde = $h*60+$m;
list( $h, $m ) = explode(':',$Final);
return ( ($h*60+$m)-$MinDesde );
}
