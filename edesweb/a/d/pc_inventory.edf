function SgADias($s){
$s = round($s/1000);
$d = floor($s/86400);
if( $d > 0 ) $s = $s - $d*86400;
$h = floor($s/3600);
if( $h > 0 ) $s = $s - $h*3600;
$m = floor($s/60);
if( $m > 0 ) $s = $s - $m*60;
if($h<10) $h = '0'.$h;
if($m<10) $m = '0'.$m;
return $d.' / '.$h.':'.$m;
}
