$tabla = "gs_conexion".$_DBUNION[0].$_DBUNION[$n];
if( qExists($tabla) ){
$newSql .= " union all ".str_replace(" gs_conexion ", " {$tabla} ", $sql);
}
}
$sql = $newSql." order by {$_DBORDER}";
