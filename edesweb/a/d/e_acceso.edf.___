[JSCheck] ?
var Desde=Hasta='';
if( eGF('_desde')!='' ){
	Desde = eDTS( eGF('_desde') );
	Desde = Desde.substr(0,4)+'-'+Desde.substr(4,2)+'-'+Desde.substr(6,2)+' 00:00:00';
}
if( eGF('_hasta')!='' ){
	Hasta = eDTS( eGF('_hasta') );
	Hasta = Hasta.substr(0,4)+'-'+Hasta.substr(4,2)+'-'+Hasta.substr(6,2)+' 23:59:59';
}

if( Desde!='' && Hasta!='' ){
	ePF('cdi','>='+Desde+'<='+Hasta);
}else if( Desde!='' ){
	ePF('cdi','>='+Desde);
}else if( Hasta!='' ){
	ePF('cdi','<='+Hasta);
}else{
	ePF('cdi','');
}
alert( eGF('cdi') );


