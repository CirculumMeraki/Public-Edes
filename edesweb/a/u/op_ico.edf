function RellenaMode(){
global $_opModeLabel;
$p = array();
$p[] = array('','');
foreach( $_opModeLabel as $k=>$v ) $p[] = array( $k, $v );
return $p;
}
function RellenaShow(){
global $_opShowLabel;
$p = array();
$p[] = array('','');
foreach( $_opShowLabel as $k=>$v ){
if( $k=='*' ) $k = ' ';
$p[] = array( $k, $v );
}
return $p;
}
